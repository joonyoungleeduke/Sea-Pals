{
  "expo": {
    "name": "deepSea",
    "slug": "the-deep-sea",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios.deepSea"
    },
    "android": {
      "package": "android.deepSea"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
